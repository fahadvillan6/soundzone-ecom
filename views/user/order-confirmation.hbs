<body>
    <div class="container mt-5">
        <div class="row align-items-center">
            <div class="col-12 mt-5">
                <div class="page-title text-center">
                    <h3> Order Confiimation</h3>
                </div>
            </div>

            <div style="width: 95%;height: 300px;" class=" ms-5 row justify-content-center align-items-center ">
                <img
                    src="data:image/png;base64,">
                <h5>Your Order Placed Successfully</h5>

            </div>

            <table class="table table-borderless " style="margin-bottom: 3rem;">
                <thead>
                    <tr>
                        <th scope="col"></th>
                        <th scope="col">Products</th>
                        <th scope="col">Quantity</th>
                        <th scope="col">Price</th>
                        {{!-- <th scope="col">Discount</th>
                        <th scope="col">total</th> --}}

                        <th scope="col">total</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data.Products}}
                    <tr>
                        <th scope="row">{{inc @index}}</th>
                        <td>{{this.Items.Name}}</td>
                        <td>{{this.Qty}}</td>
                        <td>{{this.Items.SellingPrice}}</td>



                        <td>{{mult Qty this.Items.SellingPrice}}</td>
                    </tr>
                    {{/each}}
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td>Total</td>

                        <td>{{data.TotalPrice}}</td>
                    </tr>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td>Discount</td>

                        <td>0</td>
                    </tr>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td>Total</td>

                        <td>{{data.TotalPrice}}</td>
                    </tr>
                </tbody>
            </table>
            <div class="container d-flex justify-content-center">
                <a href="/myaccount/orders" class="center btn btn-outline-primary btn-lg"> View Orders </a>
            </div>

        </div>
    </div>
</body>